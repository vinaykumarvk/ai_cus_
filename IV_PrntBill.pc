


#include "IV_Control_A.h"
EXEC SQL INCLUDE SQLCA.H;


int IV_Proc_Prnbills(	 FILE *bill,
						 FILE *fl_techlog,	
						 FILE *fl_userlog,	
						 char *chr_p_billdt,
						 char *chr_p_client,
						 SYS_IVCONTROLPARAM_STRUCT l_sys_ivcntl_param_struct,
	   				 IV_CLIENTSETUP_STRUCT l_iv_clt_setup_struct,			
		 				 char p_mastsub,
						 DEBUG_INFO_STRUCT_H **l_debug_info_ptr)
{
 

double p_totcomm=0,p_totcust=0,p_tottran=0,p_totcash=0,p_tototh=0 ;
int int_retval=0;
int int_currpos= 0;
char chr_l_buf[BUFFER_LEN];
double p_tot_portf =0;

		APL_IF_DEBUG
		{
			memset(chr_l_buf,NULL,BUFFER_LEN);
			sprintf(chr_l_buf,"The billdt is %s",chr_p_billdt);	
			CO_ProcMonitor(APL_OUT_FILE,chr_l_buf,NULL,NULL);

			memset(chr_l_buf,NULL,BUFFER_LEN);
			sprintf(chr_l_buf,"The client is  %s",chr_p_client);	
			CO_ProcMonitor(APL_OUT_FILE,chr_l_buf,NULL,NULL);

			memset(chr_l_buf,NULL,BUFFER_LEN);
			sprintf(chr_l_buf,"The MastSub is %c",p_mastsub);	
			CO_ProcMonitor(APL_OUT_FILE,chr_l_buf,NULL,NULL);
		}


   if (APL_FAILURE == IV_Proc_PrnHeader(bill,fl_userlog,chr_p_billdt,chr_p_client,l_sys_ivcntl_param_struct,l_iv_clt_setup_struct,p_mastsub,&int_currpos,l_debug_info_ptr))
	{
		   fprintf(fl_userlog,"Could not print chr_header for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
			APL_GOBACK_FAIL
	}	

	
	if (APL_FAILURE ==IV_Proc_PrnSAFEK(bill,fl_userlog,chr_p_billdt,chr_p_client,l_iv_clt_setup_struct,p_mastsub,&int_currpos,&p_tot_portf,l_debug_info_ptr))
	
	{
		fprintf(fl_userlog,"Safekeeping Details not int_found for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
	}
	p_totcust = p_tot_portf ;
   if (APL_FAILURE ==  IV_Proc_PrnCust(bill,fl_userlog,chr_p_billdt,chr_p_client,l_iv_clt_setup_struct,p_mastsub,&p_totcust,&int_currpos,l_debug_info_ptr))
	{
		fprintf(fl_userlog,"Custody Charges not int_found for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
	}

	
  if (APL_FAILURE == IV_Proc_PrnTran(bill,fl_userlog,chr_p_billdt,chr_p_client,l_iv_clt_setup_struct,p_mastsub,&p_tottran,&int_currpos,l_debug_info_ptr))
	{
		fprintf(fl_userlog,"Transaction Charges not int_found for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
	}	

   if (APL_FAILURE == IV_Proc_PrnCash(bill,fl_userlog,chr_p_billdt,chr_p_client,l_iv_clt_setup_struct,p_mastsub,&p_totcash,&int_currpos,l_debug_info_ptr))
	{
		fprintf(fl_userlog,"Cash Charges not int_found for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
	}	
   
	if (APL_FAILURE == IV_Proc_PrnOth(bill,fl_userlog,chr_p_billdt,chr_p_client,l_iv_clt_setup_struct,p_mastsub,&p_tototh,&int_currpos,l_debug_info_ptr))
	{
		fprintf(fl_userlog,"Other Charges not int_found for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);
	}	

	
	p_totcomm = p_totcust + p_tottran + p_totcash + p_tototh;


	 if (APL_FAILURE == IV_Proc_PrnFooter(bill,fl_userlog,chr_p_billdt,chr_p_client,l_sys_ivcntl_param_struct,l_iv_clt_setup_struct,p_mastsub,p_totcomm,&int_currpos,l_debug_info_ptr))
	{
		fprintf(fl_userlog,"Could not print footer for client: %s and dateof_billing :%s\n",chr_p_client,chr_p_billdt);

		APL_GOBACK_FAIL
	}
	fprintf(bill,"\f");
	fflush(bill);	


	{

			RETURN_SUCCESS:
               APL_IF_DEBUG
					{
						CO_ProcMonitor(APL_OUT_FILE,"Exiting out of IV_Proc_Prnbills Successfully\n",NULL,NULL);
					}
               return(APL_SUCCESS);

        RETURN_FAILURE:

               APL_IF_DEBUG
					{
						CO_ProcMonitor(APL_OUT_FILE,"Exiting out of IV_Proc_Prnbills UnSuccessfully\n",NULL,NULL);
					}		
					
                return (APL_FAILURE);
   }
}
