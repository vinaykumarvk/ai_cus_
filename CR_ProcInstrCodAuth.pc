		ORDER  BY  CLIENT, INSTR_CODE , LOCATION_CD ;
      curr_trd_flg = 1;
	}
	else if (int_retval == APL_RECS_EXIST) 
	{
      EXEC SQL DECLARE curr_trade1 CURSOR  FOR
		SELECT   IDENTIY_NO,CLIENT,INSTR_CODE,LOCATION_CD,DEAL_CD
		FROM     DL_DEAL
		WHERE    instr_code=RTRIM(:p_from_instr)
		AND      INSTRUMENT_DT IS NULL
