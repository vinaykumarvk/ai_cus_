      }
		else if (curr_trd_flg == 2)
		{
		EXEC SQL OPEN curr_trade1;
		}
      IS_ANY_ORA_ERROR
   
      for (;;)
         {

            if (curr_trd_flg == 1)
				{
				  EXEC  SQL FETCH curr_trade
              INTO  :h_indentity_no:i_indentity_no,
                    :h_dl_client:i_dl_client,
                    :h_instr_code:i_instr_code,
                    :h_loccode:i_loccode,
                    :h_dealcd:i_dealcd;
            }
				else if (curr_trd_flg == 2)
