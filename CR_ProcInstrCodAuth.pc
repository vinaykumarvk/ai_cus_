         IS_ANY_ORA_ERROR

         if(APL_ZERO_RESULT_SET)
                  {
                     EXEC SQL CLOSE cur_mq;
                     break ;
                  }
         if (APL_FAILURE==EI_Mod_MQData(chr_l_mq_client,
                                        chr_l_mq_refno,
                                        NULL,
                                        NULL,
                                        "T",
                                         l_debug_info_ptr))
         APL_GOBACK_FAIL

      }
      


		

		EXEC SQL INSERT INTO DL_DEALAUDIT (SELECT client,IDENTIY_NO,'T',:p_intl_env_data_struct_h_d->processtion,:p_intl_env_data_struct_h_d->usr,NULL,:chr_h_sys_access_stamp,NULL,QTY,DEAL_FRM_ORD,NULL FROM DL_DEAL WHERE ((instr_code = :p_from_instr AND (INSTRUMENT_DT IS NOT NULL) AND INSTRCONV_IND = :h_instrconv_ind AND access_stamp = :chr_h_sys_access_stamp) OR  (instr_code = :h_to_instrurity AND (INSTRUMENT_DT IS NULL) AND access_stamp = :chr_h_sys_access_stamp)));

		IS_ANY_ORA_ERROR

   }

	}
	else if (int_retval == APL_RECS_EXIST)
	{
		strcpy((char *)h_instrconv_ind, "Y");

		i_instrconv_ind = 0;


	if( int_l_pndtrdgr )
	{
		EXEC SQL UPDATE DL_DEAL
		SET INSTRCONV_IND = :h_instrconv_ind:i_instrconv_ind,
			 access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
		WHERE instr_code = :p_from_instr
		AND INSTRUMENT_DT IS NOT NULL
		AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)
		AND CLIENT NOT IN ( SELECT CLIENT FROM DL_DEAL WHERE DEAL_STAT IN ('SS','MS','UU','AA','CC','DU') 
		AND INSTR_CODE = :p_from_instr );

		IS_ANY_ORA_ERROR
		
		EXEC SQL UPDATE DL_DEAL
		SET instr_code = :h_to_instrurity:i_to_instrurity,
			 access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
		WHERE instr_code = :p_from_instr
		AND INSTRUMENT_DT IS NULL
		AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)
		AND CLIENT NOT IN ( SELECT CLIENT FROM DL_DEAL WHERE DEAL_STAT IN ('SS','MS','UU','AA','CC','DU') 
		AND INSTR_CODE = :p_from_instr );

		IS_ANY_ORA_ERROR
		
		

		EXEC SQL INSERT INTO DL_DEALAUDIT (SELECT client,IDENTIY_NO,'T',:p_intl_env_data_struct_h_d->processtion,:p_intl_env_data_struct_h_d->usr,NULL,:chr_h_sys_access_stamp,NULL,QTY,DEAL_FRM_ORD,NULL FROM DL_DEAL WHERE ((instr_code = :p_from_instr AND (INSTRUMENT_DT IS NOT NULL) AND INSTRCONV_IND = :h_instrconv_ind AND access_stamp = :chr_h_sys_access_stamp AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)) OR  (instr_code = :h_to_instrurity AND (INSTRUMENT_DT IS NULL) AND access_stamp = :chr_h_sys_access_stamp AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL))) AND CLIENT NOT IN ( SELECT CLIENT FROM DL_DEAL WHERE DEAL_STAT IN ('SS','MS','UU','AA','CC','DU') AND INSTR_CODE = :p_from_instr ) );

		IS_ANY_ORA_ERROR

	}
	else
	{
		
      if ((int_l_jocondexists > 0) && (strcmp(chr_h_to_clscode,"NTRD") == 0))
      {
             
            EXEC SQL UPDATE DL_DEAL
				SET INSTRCONV_IND = :h_instrconv_ind:i_instrconv_ind,
		          access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
      		WHERE instr_code = :p_from_instr
      		AND INSTRUMENT_DT IS NOT NULL
      		AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)
				AND CLIENT NOT IN (SELECT DISTINCT CLIENT FROM DL_DEAL WHERE INSTR_CODE = RTRIM(:p_from_instr) and DEAL_CD IN ('1','3','5','7'));
			
				IS_ANY_ORA_ERROR
		
				EXEC SQL UPDATE DL_DEAL
      		SET instr_code = :h_to_instrurity:i_to_instrurity,
          		access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
      		WHERE instr_code = :p_from_instr
      		AND INSTRUMENT_DT IS NULL
      		AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)
				AND CLIENT NOT IN (SELECT DISTINCT CLIENT FROM DL_DEAL WHERE INSTR_CODE = RTRIM(:p_from_instr) and DEAL_CD IN ('1','3','5','7'));

				IS_ANY_ORA_ERROR		
		}
		else
		{
	
				EXEC SQL UPDATE DL_DEAL
				SET INSTRCONV_IND = :h_instrconv_ind:i_instrconv_ind,
					 access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
				WHERE instr_code = :p_from_instr
				AND INSTRUMENT_DT IS NOT NULL
				AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL);

				IS_ANY_ORA_ERROR

		
				EXEC SQL UPDATE DL_DEAL
				SET instr_code = :h_to_instrurity:i_to_instrurity,
			 		access_stamp = :chr_h_sys_access_stamp:i_sys_access_stamp
				WHERE instr_code = :p_from_instr
				AND INSTRUMENT_DT IS NULL
				AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL);

				IS_ANY_ORA_ERROR
		}

     
      EXEC SQL DECLARE  cur_mq1 CURSOR FOR
      SELECT CLIENT,IDENTIY_NO FROM DL_DEAL
      WHERE INSTR_CODE = :h_to_instrurity
		AND ACCESS_STAMP = :chr_h_sys_access_stamp
      AND INSTRUMENT_DT IS NULL
		AND DEAL_STAT != 'UU'
      AND EXISTS (SELECT cln_code FROM MT_CLIENT WHERE MT_CLIENT.cln_code = DL_DEAL.CLIENT
      AND STATUS NOT IN (:chr_l_acc_status1,:chr_l_acc_status2));

      IS_ANY_ORA_ERROR

      EXEC SQL OPEN cur_mq1;
      IS_ANY_ORA_ERROR
      for(;;)
        {
         memset(chr_l_mq_client,APL_NULL_CHAR,APL_CLIENT_LENGTH);
         memset(chr_l_mq_refno,APL_NULL_CHAR,17);
         EXEC SQL FETCH cur_mq1 into :chr_l_mq_client:i_mq_account ,:chr_l_mq_refno:i_mq_refno ;
            IS_ANY_ORA_ERROR

         if(APL_ZERO_RESULT_SET)
                  {
                     EXEC SQL CLOSE cur_mq1;
                     break ;
                  }
         if (APL_FAILURE==EI_Mod_MQData(chr_l_mq_client,
                                        chr_l_mq_refno,
                                        NULL,
                                        NULL,
                                        "T",
                                         l_debug_info_ptr))
             APL_GOBACK_FAIL

        }
   	
		
		

		EXEC SQL INSERT INTO DL_DEALAUDIT (SELECT client,IDENTIY_NO,'T',:p_intl_env_data_struct_h_d->processtion,:p_intl_env_data_struct_h_d->usr,NULL,:chr_h_sys_access_stamp,NULL,QTY,DEAL_FRM_ORD,NULL FROM DL_DEAL WHERE ((instr_code = :p_from_instr AND (INSTRUMENT_DT IS NOT NULL) AND INSTRCONV_IND = :h_instrconv_ind AND access_stamp = :chr_h_sys_access_stamp AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL)) OR  (instr_code = :h_to_instrurity AND (INSTRUMENT_DT IS NULL) AND access_stamp = :chr_h_sys_access_stamp AND client IN (SELECT client FROM DL_INSTRUMENTCONV WHERE frm_instr_code=:p_from_instr AND CHNG_DT IS NULL))));

		IS_ANY_ORA_ERROR
		
	}

	}




   
   EXEC SQL COMMIT;
   IS_ANY_ORA_ERROR			

	APL_GOBACK_SUCCESS

	RETURN_SUCCESS :
	{
		CO_ProcMonitor(p_ptr_log_file,"Leaving processtion CR_Mod_TrdInstrConv successfully \n",NULL,NULL);
		return(APL_SUCCESS);
	}

	RETURN_FAILURE :
	{
		CO_ProcMonitor(p_ptr_log_file,"Leaving processtion CR_Mod_TrdInstrConv with failure \n",NULL,NULL);
		return(APL_FAILURE);

	}
}



